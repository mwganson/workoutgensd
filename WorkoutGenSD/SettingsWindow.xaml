<Window x:Class="WorkoutGenSD.SettingsWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="WorkoutGenSD Application Settings" Height="512.5" Width="403.75" FontSize="10.5">
    <Grid Height="482.5" Width="391.25">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="302*" />
            <ColumnDefinition Width="34*" />
            <ColumnDefinition Width="42*" />
        </Grid.ColumnDefinitions>
        <Label Height="25" Margin="107,0,0,0" Name="label1" VerticalAlignment="Top">Workout Lengths (in minutes)</Label>
        <TextBox Name="workoutLengthBox" TextChanged="workoutLengthBox_TextChanged" Height="21" VerticalAlignment="Top" Grid.Column="1" />
