        <Button  Click="buttonCancel_Click" Margin="0,0,33.75,21.25" Name="buttonCancel" HorizontalAlignment="Right" Width="50" Grid.ColumnSpan="2" Height="25" VerticalAlignment="Bottom">Cancel</Button>
        <Button Click="defaultsButton_Click" HorizontalAlignment="Right" Margin="0,0,95,21.25" Name="defaultsButton" Width="50" Height="25" VerticalAlignment="Bottom">Defaults</Button>
        <TextBox Height="21" Margin="0,20,0,0" Name="unknownBlock02Exp1" VerticalAlignment="Top" Grid.Column="1" />
        <TextBox Height="21" Margin="0,40,0,0" Name="unknownBlock08Exp1" VerticalAlignment="Top" Grid.Column="1" />
        <TextBox Height="21" Margin="0,40,9,0" Name="unknownBlock08Exp2" VerticalAlignment="Top" Grid.Column="2" />
        <TextBox Height="21" Margin="0,60,0,0" Name="unknownBlock10Exp1" VerticalAlignment="Top" Grid.Column="1" />
        <TextBox Height="21" Margin="0,80,0,0" Name="unknownBlock11Exp1" VerticalAlignment="Top" Grid.Column="1" />
        <TextBox Height="21" Margin="0,100,0,0" Name="unknownBlock12Exp1" VerticalAlignment="Top" Grid.Column="1" />
        <TextBox Height="21" Margin="0,120,0,0" Name="unknownBlock13Exp1" VerticalAlignment="Top" Grid.Column="1" />
