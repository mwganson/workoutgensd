<Window x:Class="WorkoutGenSD.InsertWaveFileWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="InsertWaveFileWindow" Height="186" Width="300">
    <Grid Height="154">
        <Label Height="27" Margin="12,29,88,0" Name="label1" VerticalAlignment="Top">Play at which second into interval?</Label>
        <ComboBox Height="24.8" HorizontalAlignment="Right" Margin="0,31,26,0" Name="comboBox1" VerticalAlignment="Top" Width="56" />
        <Button Click="buttonOK_Click" Height="23" Margin="68,0,135,14" Name="buttonOK" VerticalAlignment="Bottom">OK</Button>
        <Button Click="buttonCancel_Click" Height="23" HorizontalAlignment="Right" Margin="0,0,42,14" Name="buttonCancel" VerticalAlignment="Bottom" Width="75">Cancel</Button>
        <Label Margin="12,61,12,71" Name="label2">Note: Multiple files may be placed at :00 seconds.</Label>
        <Label Height="22" Margin="12,0,12,53" Name="label3" VerticalAlignment="Bottom">They will be played in the order they are placed.</Label>
    </Grid>
</Window>
